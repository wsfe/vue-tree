{
  "name": "@wsfe/vue-tree",
  "version": "3.0.9",
  "types": "./types",
  "description": "A vue tree component using virtual list.",
  "main": "./dist/v3/vue-tree.umd.js",
  "module": "./dist/v3/vue-tree.mjs",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/v3/vue-tree.mjs",
      "require": "./dist/v3/vue-tree.umd.js"
    },
    "./style.css": "./dist/v3/style.css",
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite",
    "dts": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "npm run dts && npm run build:2 && npm run build:3",
    "build:2": "pnpm --filter vue2 run build",
    "build:3": "vite build",
    "docs": "vue-tsc --noEmit && vite build -c examples/vite.config.ts",
    "preview": "vite preview",
    "test": "jest --watch",
    "prettier": "prettier --write \"{src,examples,tests}/**/*.{ts,js,json,vue,tsx,less,scss,less,html}\" --fix",
    "postinstall": "node ./scripts/postinstall.cjs"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "scripts",
    "src",
    "types"
  ],
  "author": {
    "name": "ChuChencheng",
    "url": "https://github.com/ChuChencheng"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "tree",
    "select",
    "tree select",
    "virtualtree",
    "virtual tree",
    "virtual-tree",
    "vue-tree",
    "vue tree",
    "vue tree component",
    "虚拟树"
  ],
  "homepage": "https://github.com/wsfe/vue-tree",
  "license": "MIT",
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^8.0.2",
    "@types/jest": "26.0.14",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/babel-preset-app": "^5.0.8",
    "@vue/test-utils": "2.0.0-rc.18",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^26.6.3",
    "jest": "26.6.3",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "less": "^4.1.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "ts-jest": "26.5.6",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vue": "^3.2.47",
    "vue-jest": "5.0.0-alpha.10",
    "vue-tsc": "^1.0.24"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "pnpm": {
    "packageExtensions": {
      "vue-template-compiler@2.6.14": {
        "peerDependencies": {
          "vue": "2.6.14"
        }
      }
    }
  }
}